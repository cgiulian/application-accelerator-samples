{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Steeltoe": "Warning",
      "Sample": "Information"
    }
  },
  "Management": {
    "Endpoints": {
      "Metrics":{
        "ScrapeResponseCacheDurationMilliseconds": "5000"
        }
      }
  },
  "Management": {
    "Endpoints": {
      "Actuator":{
        "Exposure": {
          "Include": [ "*" ]
        }
      }
    }
  },
  "Management": {
    "Metrics": {
      "Observer":{
        "EventCounterEvents": true,
        "IncludedMetrics": [
          "cpu-usage"
        ]
      }
    }
  }
}
